{
    "name": "shabbyrobe/cachet",
    "abandoned": "symfony/cache",
    "description": "Pluggable Caching for PHP -- NO LONGER DEVELOPED, use symfony/cache",
    "keywords": ["php", "caching", "cache"],
    "license": "MIT",
    "homepage": "http://k3jw.com/code/cachet/",
    "authors": [
        {
        	"name": "Blake Williams", 
        	"email": "code@k3jw.com"
        }
    ],
    "require": {
    	"php": ">=7.2.0",
      "psr/simple-cache": "^1.0.1"
    }, 
    "require-dev": {
    	"phpunit/phpunit": "9.5.5"
    },
    "provide": {
      "psr/simple-cache-implementation": "1.0"
    }, 
    "autoload": {
     	"psr-4": { "Cachet\\": "src/" }
    },
    "autoload-dev": {
     	"psr-4": { "Cachet\\Test\\": "test/lib" }
    }
}
